{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/__azurite_db_blob__.json": true,
    "**/__azurite_db_queue__.json": true,
    "**/__azurite_db_queue_extent__.json": true,
    "**/__blobstorage__": true,
    "**/__pycache__": true,
    "**/__queuestorage__": true,
    "**/.classpath": true,
    "**/.factorypath": true,
    "**/.idea": true,
    "**/.ipynb_checkpoints": true,
    "**/.mypy_cache": true,
    "**/.project": true,
    "**/.pytest_cache": true,
    "**/.settings": true,
    "**/.venv": true,
    "**/.vs": true,
    "**/.vscode": true,
    "**/.vscode/solution-explorer": true,
    "**/*.code-workspace": true,
    "**/*.egg-info": true,
    "**/bin": true,
    "**/obj": true,
    "**/out": false,
    "**/sqltools*.log": true,
    "**/target": true,
    ".metals": true
  },
  "files.watcherExclude": {
    "**/target": true
  },

  "cSpell.words": [
    "allstats",
    "CEQ",
    "CGE",
    "CGT",
    "CLE",
    "CLT",
    "CNE",
    "fd",
    "fdt",
    "fdtu",
    "fdu",
    "fnow",
    "ftoday",
    "futcnow",
    "futctoday",
    "IEQ",
    "IGE",
    "ILE",
    "ILT",
    "INE",
    "semver",
    "skipindex",
    "skiplast",
    "stddev",
    "utcnow",
    "utctoday",
    "Ver"
  ],

  "powershell.cwd": "${workspaceFolder}",
  "[powershell]": {
    "editor.suggest.showSnippets": false,
  }
}
